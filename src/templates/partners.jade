extends ./_layout.jade

block append globals
	- var url = 'partners.html'
block menu
	+menu({active: 'partners'})

block content
	.section
		.section__head
			h2.section__title Поставляемое оборудование
		.section__body
			.pure-content.wrapper
				table
					thead
						tr
							th Наименование
							th Оборудование
							th(width='100') Сайт
					tbody
						tr
							th EWM
							td EWM - инверторное сварочное оборудование MIG/MAG/TIG/MMA/Plasma/Microplasma (МП/МАДП/РАД/РД),  классические сварочные полуавтоматы MIG/MAG (МП/МАДП), аппараты для точечной контактной сварки, сварочные горелки MIG/TIG (МП/РАД), аксессуары (маски, устройства для заточки вольфрама, устройства документирования, запасные части.
							td
								a(href="http://www.ewm-russia.ru", target="_blank") ewm-russia.ru
						tr
							th KEMPPI
							td KEMPPI - инверторное сварочное оборудование MIG/MAG/TIG/MMA (МП/МАДП/РАД/РД),   сварочные горелки MIG/TIG (МП/РАД), аксессуары (маски, запасные части).
							td
								a(href="http://www.kemppi.com", target="_blank") kemppi.com
						tr
							th ESAB
							td ESAB - инверторное сварочное оборудование MIG/MAG/TIG/MMA/Plasma (МП/МАДП/РАД/РД),  классические сварочные полуавтоматы MIG/MAG (МП/МАДП), оборудование для плазменной резки, сварочные горелки MIG/TIG (МП/РАД), аксессуары (электрододержатели, маски, запасные части)
							td
								a(href="http://www.esab.ru", target="_blank") esab.ru
						tr
							th BLUEWELD (Блювелд)
							td BLUEWELD - оборудование для ММА (РД) сварки, сварочные полуавтоматы MIG/MAG (МП/МАДП), аппараты для TIG (РАД) сварки,  аппараты для плазменной резки, аппараты для точечной контактной сварки, аксессуары, а также пуско-зарядные устройства.
							td
								a(href="http://www.blueweld.ru", target="_blank") blueweld.ru
						tr
							th HDB
							td Средства малой механизации, комплексные устанвоки для автоматизации сварочного процесса
							td
								a(href="http://www.hdb-welding.com", target="_blank") hdb-welding.com
						tr
							th ПТК
							td Электросварочное оборудование MIG/MAG/TIG/MMA/Plasma (МП/МАДП/РАД/РД), газосварочное оборудование, промышленное оборудование, сварочные горелки EvoSpark, аксессуары и средства защиты, расходные материалы
							td
								a(href="http://www.ptk-spb.ru", target="_blank") ptk-spb.ru
						tr
							th ИТС
							td Выпрямители сварочные, сварочные полуавтоматы, реостаты балластные, тренажеры сварщика, оборудование для подготовки кромок, газосварочное оборудование
							td
								a(href="http://www.ets-svarka.ru", target="_blank") ets-svarka.ru
						tr
							th НПП ВРТ
							td Устройства для подогрева защитного газа (СО2, смеси), блоки питания для устройств подогрева защитного газа
							td
								a(href="http://nppvrt.ru", target="_blank") nppvrt.ru
						tr
							th ТЭН и К плюс
							td Электропечи и термопеналы для сушки и прокалки сварочных электродов, сварочного флюса
							td
								a(href="http://tenik-miass.ru", target="_blank") tenik-miass.ru
						tr
							th TBI
							td TBi Industries GmbH – ведущий мировой производитель высококачественных комплектующих для сварочного оборудования. Основным направлением деятельности компании является производство горелок для MIG/MAG (МП/МАДП), TIG (РАД) - сварки, горелок для плазменной сварки и резки, роботизированных систем и продукции High-End.
							td
								a(href="http://www.tbi-industries.com", target="_blank") tbi-industries.com
						tr
							th ABICOR
							td ABICOR BINZEL – ведущий мировой производитель высококачественных комплектующих для сварочного оборудования. Основным направлением деятельности компании является производство горелок для MIG/MAG (МП/МАДП), TIG (РАД) - сварки, горелок для плазменной сварки и резки, роботизированных систем и продукции High-End.
							td
								a(href="http://www.binzel-abicor.com/RU/rus", target="_blank") binzel-abicor.com
						tr
							th АГНИ
							td Сварочные горелки для:
								br
								| - аргонодуговой сварки с вольфрамовым электродом TIG (РАД);
								br
								| - механизированной сварки MIG/MAG (МП/МАДП);
								br
								| - сменные части и запчасти горелок;
								br
								| - присоединительные элементы для горелок.

							td
								a(href="http://www.agniru.ru", target="_blank") agniru.ru
						tr
							th TERMACUT
							td Заменяемые расходные материалы, аксессуары и плазмотроны для плазменной резки, MIG/TIG (МП/РАД) сварки. Более чем 150 заменяемых моделей плазмотронов с сопутствующими и расходными материалами.
							td
								a(href="http://thermacut.ru", target="_blank") thermacut.ru
						tr
							th УРАЛЭЛЕКТРОМЕДЬ
							td Контактные наконечники ДУКМ
							td
								a(href="http://www.elem.ru", target="_blank") elem.ru
						tr
							th МЕТАЛЛСТРОЙСНАБ
							td Сварочная проволока для механизированной сварки конструкций ответственного и общего назначения в среде защитных газов Ar/CО2- Ar/CО2/O2- CO2 омедненная CВ08Г2С-О/ CВ08ГС-О, полированная CВ08Г2С-П/ CВ08ГС-П диаметрами 0,8-1,6 мм. Соответствует  требованиям ГОСТа 2246-70 и ТУ BY100172845.004-2010
							td
								a(href="http://www.metallstroysnab.ru", target="_blank") metallstroysnab.ru
						tr
							th СВАРКОМПЛЕКТ
							td Проволока, пруток для нержавеющей стали Sandvik (Швеция).
							td
								a(href="http://www.svarka-ufa.ru", target="_blank") svarka-ufa.ru
						tr
							th Хобекс
							td Более 120-ти марок сварочных электродов в ассортименте по диаметрам и номенклатуре.
							td
								a(href="http://www.hobex34.ru", target="_blank") hobex34.ru
